config LCZ_MEMFAULT
    bool "Enable Memfault integration"
	select MEMFAULT
	select MEMFAULT_HTTP_ENABLE

if LCZ_MEMFAULT

config LCZ_MEMFAULT_LOG_LEVEL
	int "Memfault log level"
	default 4
	range 0 4

endif # LCZ_MEMFAULT
